{
    "name": "widop/framework-extra-bundle",
    "description": "An extension to Symfony2 FrameworkBundle that adds annotation configuration for Controller classes.",
    "keywords": [ "annotations","controllers" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Widop",
            "email": "contact@widop.com"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.1",
        "doctrine/common": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Widop\\FrameworkExtraBundle": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Widop/FrameworkExtraBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
